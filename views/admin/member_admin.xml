<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="member_action" model="ir.actions.act_window">
    <field name="name">member</field>
    <field name="res_model">library_system.member</field>
    <field name="view_mode">tree,form</field>
  </record>

  <record id="ls_member_view_tree" model="ir.ui.view">
    <field name="name">member</field>
    <field name="model">library_system.member</field>
    <field name="arch" type="xml">
      <tree string="member">
        <field name="nis"></field>
        <field name="full_name"></field>
        <field name="kelas"></field>
      </tree>
    </field>
  </record>

  <record id="ls_member_view_form" model="ir.ui.view">
    <field name="name">library_system.member.view.form</field>
    <field name="model">library_system.member</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="nis"></field>
            <field name="full_name"></field>
            <field name="kelas"></field>
          </group>
        </sheet>
      </form>
    </field>
  </record>

</odoo>